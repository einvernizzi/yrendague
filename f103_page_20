wwv_flow_api.create_page_process(
 p_id=>wwv_flow_api.id(26054456011418538)
,p_process_sequence=>10
,p_process_point=>'ON_DEMAND'
,p_process_type=>'NATIVE_PLSQL'
,p_process_name=>'DOWNLOAD_FILE'
,p_process_sql_clob=>wwv_flow_string.join(wwv_flow_t_varchar2(
'declare',
'  --',
'  l_mime_type varchar2(100) := ''application/octet-stream'';',
'  l_file_id   number;',
'  l_ubicacion varchar2(20);',
'  l_file_name varchar2(100);',
'  l_length    number;',
'  l_blob      blob;',
'  --',
'  cursor l_cur is',
'    select arch_nombre,',
'           arch_mime_type,',
'           arch_contenido',
'      from finm001_archivos_v',
'     where seq_id = l_file_id;',
'  --',
'begin',
'  -- set file_id',
'  l_file_id       := apex_application.g_x01;',
'    ',
'  -- get clob from collection',
'  open l_cur;',
'  fetch l_cur',
'    into l_file_name, l_mime_type, l_blob;',
'  close l_cur;',
'  --',
'  l_length := dbms_lob.getlength(l_blob);',
'  --',
unistr('  -- Download wenn BLOB gef\00FCllt'),
'  if l_length is not null then',
'    -- set up HTTP header',
'    owa_util.mime_header(l_mime_type, false);',
'    -- set the size so the browser knows how much to download',
'    htp.p(''Content-length: '' || l_length);',
'    -- the filename will be used by the browser if the users does a save as',
'    htp.p(''Content-Disposition:  inline; filename="'' || l_file_name || ''"'');',
'    -- close the headers            ',
'    owa_util.http_header_close;',
'    -- download the BLOB',
'    wpg_docload.download_file(l_blob);',
'  else',
'    htp.p(''Content-length: '' || l_length);',
'    -- the filename will be used by the browser if the users does a save as',
'    htp.p(''Content-Disposition:  inline; filename="'' || l_file_name || ''"'');',
'    owa_util.http_header_close;',
'    htp.p(''El fichero esta vacio'');',
'  end if;',
'  --',
'end;',
''))
,p_error_display_location=>'INLINE_IN_NOTIFICATION'
);
