'IF :P20_CONF_MOSTRAR_OCULTAR_DV = ''S'' THEN',
'  select SUBSTR(rso_ruc, INSTR(rso_ruc,''-'',1) -8,7), SUBSTR(rso_ruc, INSTR(rso_ruc,''-'',1) +1,1)',
'    into :P20_RSO_RUC_AUX, :P20_RSO_DV',
'    from GEN_RAZON_SOCIAL',
'   where rso_empr = :p_empresa_id',
'     and rso_codigo = :P20_RSO_CODIGO;',
'ELSE',
'  :P20_RSO_RUC_AUX := :P20_RSO_RUC;',
'END IF;'))
