'function pjs_contenedor_foto(p_item){',
'    return document.getElementById(p_item + ''_CONTAINER'').getElementsByClassName(''t-Form-itemWrapper'')[0];',
'}',
'',
'function pjs_contenedor_firma(p_item){',
'    return document.getElementById(p_item + ''_CONTAINER'').getElementsByClassName(''t-Form-itemWrapper'')[0];',
'}',
'',
'function pjs_crear_item_foto(p_item, p_url){',
'    var v_item_fp;',
'    v_item_fp = document.createElement("img");',
'    v_item_fp.setAttribute("id", p_item);',
'    v_item_fp.setAttribute("src", p_url);',
'    v_item_fp.setAttribute("alt", "Imagen");',
'    v_item_fp.setAttribute("title", "Imagen");',
'    v_item_fp.setAttribute("class", "display_image apex-item-image foto-perfil");',
'    v_item_fp.setAttribute("onclick", "javascript:pjs_cargar_foto();");',
'    ',
'    return v_item_fp;',
'}',
'',
'',
'function pjs_crear_item_firma(p_item, p_url){',
'    var v_item_fp;',
'    v_item_fp = document.createElement("img");',
'    v_item_fp.setAttribute("id", p_item);',
'    v_item_fp.setAttribute("src", p_url);',
'    v_item_fp.setAttribute("alt", "Imagen");',
'    v_item_fp.setAttribute("title", "Imagen");',
'    v_item_fp.setAttribute("class", "display_image apex-item-image foto-firma");',
'    v_item_fp.setAttribute("onclick", "javascript:pjs_cargar_firma();");',
'    ',
'    return v_item_fp;',
'}',
'',
'function pjs_cambiar_foto(p_item) {',
'    try{',
'        var v_item_fp = document.getElementById(p_item);',
'        if (!v_item_fp){',
'            var v_parent_item = pjs_contenedor_foto(p_item);',
'            v_item_fp = pjs_crear_item_foto(p_item, "#WORKSPACE_IMAGES#cc_img/perfil-vacio.jpg");',
'            v_parent_item.appendChild(v_item_fp);',
'            //pjs_mensaje_exitoso(v_parent_item.innerHTML, 20000);',
'        }',
'        if (v_item_fp){',
'            if (v_item_fp.src == null){',
'                v_item_fp.src = ''#WORKSPACE_IMAGES#cc_img/perfil-vacio.jpg'';',
'            }',
'        }',
'    }',
'    catch (err ){',
'    }',
'}',
'',
'',
'',
'function pjs_cambiar_firma(p_item) {',
'    try{',
'        var v_item_fp = document.getElementById(p_item);',
'        if (!v_item_fp){',
'            var v_parent_item = pjs_contenedor_firma(p_item);',
'            v_item_fp = pjs_crear_item_firma(p_item, "#WORKSPACE_IMAGES#FIRMA_VACIO/FIRMA_VACIO.jpg");',
'            v_parent_item.appendChild(v_item_fp);',
'            //pjs_mensaje_exitoso(v_parent_item.innerHTML, 20000);',
'        }',
'        if (v_item_fp){',
'            if (v_item_fp.src == null){',
'                v_item_fp.src = ''#WORKSPACE_IMAGES#FIRMA_VACIO/FIRMA_VACIO.jpg'';',
'            }',
'        }',
'    }',
'    catch (err ){',
'    }',
'}',
'',
'function pjs_recargar_foto(p_item){ ',
'    var v_url = $x(p_item + "_URL").value;',
'    var v_parent_item = pjs_contenedor_foto(p_item);',
'    v_parent_item.removeChild(v_parent_item.childNodes[0]);',
'    var v_item_fp = pjs_crear_item_foto(p_item, v_url);',
'    v_parent_item.appendChild(v_item_fp);',
'}',
'',
'function pjs_recargar_firma(p_item){',
'    var v_url = $x(p_item + "_URL").value;',
'    var v_parent_item = pjs_contenedor_firma(p_item);',
'    v_parent_item.removeChild(v_parent_item.childNodes[0]);',
'    var v_item_fp = pjs_crear_item_firma(p_item, v_url);',
'    v_parent_item.appendChild(v_item_fp);',
'}',
'',
'',
'',
'',
'function pjs_cargar_foto(){',
unistr('    // Creamos el script para eliminar la configuraci\00F3n anterio y crear una nueva para el Dropzone'),
'    var v_sql = "begin ";',
'    v_sql += "  delete gen_archivo_conf_dz where acd_usuario = upper(''&APP_USER.''); ";',
'    v_sql += "  insert into gen_archivo_conf_dz(acd_usuario, acd_mime_type, acd_max_files, acd_max_size) values(''&APP_USER.'',''image/*'', 1, 2); ";',
'    v_sql += "end;";',
'    ',
'    // Ejecutamos el script creado previamente',
'    pjs_gen_exec(v_sql, "P20_PERFIL");',
'    ',
unistr('    // Abrimos el cargador de im\00E1genes, indicando que de confirmarse, debe reemplazar los valores en la colecci\00F3n'),
'    pjs_open_modal_dialog(271, ''P271_COLLECT,P271_ACCION,P271_TIPO_ARCHIVO,P271_ITEM_PARA_RECARGAR'',''FINM001_ARCHIVOS,R,FP,P20_PERFIL'', ''regionFotoPerfil'');',
'}',
'',
'function pjs_cargar_firma(){',
