wwv_flow_api.create_page_da_event(
 p_id=>wwv_flow_api.id(7674192199132851)
,p_name=>'AD_DEFINIR_ANTIGUEDAD'
,p_event_sequence=>780
,p_triggering_element_type=>'ITEM'
,p_triggering_element=>'P20_ANTIG_MESES,P20_ANTIG_ANHOS'
,p_bind_type=>'bind'
,p_bind_event_type=>'change'
);
wwv_flow_api.create_page_da_action(
 p_id=>wwv_flow_api.id(7674503492132853)
,p_event_id=>wwv_flow_api.id(7674192199132851)
,p_event_result=>'TRUE'
,p_action_sequence=>10
,p_execute_on_page_init=>'N'
,p_action=>'NATIVE_EXECUTE_PLSQL_CODE'
,p_attribute_01=>wwv_flow_string.join(wwv_flow_t_varchar2(
'DECLARE',
'V_DIAS NUMBER;',
'V_FEC_SALIDA DATE;',
'BEGIN',
'',
'BEGIN',
' SELECT RSO_FEC_SALIDA',
'   INTO V_FEC_SALIDA',
'   FROM GEN_RAZON_SOCIAL',
'  WHERE RSO_CODIGO = :P20_RSO_CODIGO',
'    AND RSO_IND_EMPLEADO = ''S''',
'    AND RSO_EMPR = 43;',
' EXCEPTION',
' WHEN NO_DATA_FOUND THEN',
' V_FEC_SALIDA := NULL;',
' END;',
'',
'    /*IF :P20_ANTIG_ANHOS = 0 AND :P20_ANTIG_MESES = 0 AND :P20_RSO_FEC_SALIDA IS NOT NULL THEN',
'       V_DIAS := TO_DATE(:P20_RSO_FEC_SALIDA, ''DD/MM/YYYY'') - TO_DATE(:P20_RSO_FEC_INGRESO, ''DD/MM/YYYY'');',
unistr('       :P20_ANTIGUEDAD := V_DIAS ||'' D\00EDas'';*/'),
'   /* IF :P20_ANTIG_ANHOS = 0 AND :P20_ANTIG_MESES = 0 AND :P20_RSO_FEC_SALIDA IS NULL THEN',
'        V_DIAS := TO_DATE(SYSDATE, ''DD/MM/YYYY'') - TO_DATE(:P20_RSO_FEC_INGRESO, ''DD/MM/YYYY'');',
unistr('       :P20_ANTIGUEDAD := V_DIAS ||'' D\00EDas'';*/'),
'   -- ELSE',
'   ',
'       IF :P20_RSO_FEC_SALIDA IS NOT NULL THEN',
'           IF :P20_RSO_CODIGO = 399 THEN',
'                 V_DIAS := f_dias(:P20_RSO_FEC_INGRESO,V_FEC_SALIDA)+1;',
'           ELSE',
'                 V_DIAS := f_dias(:P20_RSO_FEC_INGRESO,V_FEC_SALIDA);',
'           END IF;',
'           ',
unistr('       :P20_ANTIGUEDAD :=  :P20_ANTIG_ANHOS ||'' A\00F1os ''||:P20_ANTIG_MESES||'' Meses '' || V_DIAS ||'' D\00EDas'';'),
'       ',
'      ELSE',
'         V_DIAS := f_dias(:P20_RSO_FEC_INGRESO, SYSDATE);',
'         ',
unistr('       :P20_ANTIGUEDAD :=  :P20_ANTIG_ANHOS ||'' A\00F1os ''||:P20_ANTIG_MESES||'' Meses '' || V_DIAS ||'' D\00EDas'';'),
'      END IF;',
'  --  END IF;',
'END;',
''))
