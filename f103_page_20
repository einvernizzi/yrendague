,p_process_sequence=>50
,p_process_point=>'AFTER_SUBMIT'
,p_process_type=>'NATIVE_PLSQL'
,p_process_name=>'Insert FIN_PROV_EMPRESA_INS'
,p_process_sql_clob=>wwv_flow_string.join(wwv_flow_t_varchar2(
'DECLARE',
'  V_EX VARCHAR2(1);',
'BEGIN',
'  IF nvl(:P20_RSO_IND_PROVEEDOR, ''N'') = ''S'' AND :P20_RSO_MON IS NOT NULL THEN',
'    BEGIN',
'      SELECT DISTINCT ''S''',
'        INTO V_EX',
'        FROM FIN_PROV_EMPRESA PE',
'       WHERE PE.PREM_EMPR = :P_EMPRESA_ID',
'         AND PE.PREM_PROV = :P20_RSO_CODIGO;',
'    EXCEPTION',
'      WHEN NO_DATA_FOUND THEN',
'        NULL;',
'    END;',
'  ',
'    IF NVL(V_EX, ''N'') = ''N'' THEN',
'      BEGIN',
'        INSERT INTO FIN_PROV_EMPRESA',
'          (SELECT :P_EMPRESA_ID,',
'                  :P20_RSO_CODIGO,',
'                  M.MON_CODIGO,',
'                  NULL            PREM_FEC_PRI_OPER,',
'                  NULL            PREM_FEC_ULT_OPER,',
'                  0               PREM_IMP_MAY_OPER_LOC,',
'                  0               PREM_IMP_MAY_OPER_MON,',
'                  NULL            PREM_FEC_MAY_OPER,',
'                  0               PREM_DB_PER_ACT_LOC,',
'                  0               PREM_DB_PER_ACT_MON,',
'                  0               PREM_CR_PER_ACT_LOC,',
'                  0               PREM_CR_PER_ACT_MON,',
'                  0               PREM_DB_PER_SGTE_LOC,',
'                  0               PREM_DB_PER_SGTE_MON,',
'                  0               PREM_CR_PER_SGTE_LOC,',
'                  0               PREM_CR_PER_SGTE_MON,',
'                  0               PREM_SALDO_ANT_LOC,',
'                  0               PREM_SALDO_ANT_MON,',
'                  0               PREM_SALDO_ACT_LOC,',
'                  0               PREM_SALDO_ACT_MON,',
'                  SYSDATE         PREM_FEC_GRAB,',
'                  NULL            PREM_LUGAR_ORIGEN_REPLICA',
'             FROM GEN_MONEDA M',
'            WHERE M.MON_EMPR = :P_EMPRESA_ID);',
'      EXCEPTION',
'        WHEN DUP_VAL_ON_INDEX THEN',
'          NULL;',
'      END;',
'    END IF;',
'  ',
'  ELSIF NVL(:P20_RSO_IND_PROVEEDOR, ''N'') = ''S'' AND :P20_RSO_MON IS NULL THEN',
'    APEX_ERROR.ADD_ERROR(P_MESSAGE          => ''Debe seleccionar una moneda!.'',',
'                         P_DISPLAY_LOCATION => APEX_ERROR.C_ON_ERROR_PAGE);',
'  END IF;',
