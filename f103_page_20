wwv_flow_api.create_page_process(
 p_id=>wwv_flow_api.id(7576574957029997)
,p_process_sequence=>80
,p_process_point=>'AFTER_SUBMIT'
,p_process_type=>'NATIVE_PLSQL'
,p_process_name=>'PP_INSERTAR_FIN_CLI_EMPRESA'
,p_process_sql_clob=>wwv_flow_string.join(wwv_flow_t_varchar2(
'declare',
'v_porc_interes varchar2(4); --NUMBER(5,2);',
'  cursor c_limite is',
'  select n001 CLEM_MON,',
'    	 n002 CLEM_IMP_LIM_CR,',
'    	 c001 CLEM_BLOQ_LIM_CR,',
'    	 n004 CLEM_PLAN_FINAN,',
'    	 n005 CLEM_PORC_DESCUENTO,',
'    	 c006 CLEM_NRO_LISTA_PRECIO,',
'    	 c007 CLEM_RECARGO,',
'    	 c008 CLEM_PORC_INTERES_MOROSIDA',
'  from apex_collections ',
'  where collection_name = ''FINM01_AGREGAR_LIMITE_CREDITO'';',
'',
'',
'begin',
'   delete from fin_cli_empresa',
'   where clem_cli = :P20_RSO_CODIGO',
'   and clem_empr = :P_EMPRESA_ID;',
'   ',
'   for s in c_limite loop',
'   /*select replace(s.CLEM_PORC_INTERES_MOROSIDA, ''.,'')',
'   into v_porc_interes',
'   from dual;*/',
'     begin',
'       insert into fin_cli_empresa(CLEM_EMPR,',
'                                   CLEM_CLI,',
'                                   CLEM_MON,',
'                                   CLEM_IMP_LIM_CR,',
'                                   CLEM_BLOQ_LIM_CR,',
'                                   CLEM_PLAN_FINAN,',
'                                   CLEM_PORC_DESCUENTO,',
'                                   CLEM_NRO_LISTA_PRECIO,',
'                                   CLEM_RECARGO,',
'                                   CLEM_PORC_INTERES_MOROSIDAD)',
'       values (:P_EMPRESA_ID, :P20_RSO_CODIGO, s.CLEM_MON, s.CLEM_IMP_LIM_CR, s.CLEM_BLOQ_LIM_CR, s.CLEM_PLAN_FINAN, s.CLEM_PORC_DESCUENTO, s.CLEM_NRO_LISTA_PRECIO, s.CLEM_RECARGO,s.CLEM_PORC_INTERES_MOROSIDA/*v_porc_interes*/);',
'     exception',
'       when DUP_VAL_ON_INDEX then',
'        null; ',
'     end;',
'   end loop;',
'',
'exception ',
'when others then',
'    raise_application_error(-20101, chr(10) || chr(10) || sqlerrm || chr(10) || chr(10));',
'end;'))
,p_error_display_location=>'INLINE_IN_NOTIFICATION'
,p_process_when=>'CREATE,SAVE'
,p_process_when_type=>'REQUEST_IN_CONDITION'
);
