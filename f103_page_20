wwv_flow_api.create_page_da_action(
 p_id=>wwv_flow_api.id(7581732199177088)
,p_event_id=>wwv_flow_api.id(7581393307177086)
,p_event_result=>'TRUE'
,p_action_sequence=>10
,p_execute_on_page_init=>'N'
,p_action=>'NATIVE_EXECUTE_PLSQL_CODE'
,p_attribute_01=>wwv_flow_string.join(wwv_flow_t_varchar2(
'DECLARE',
'cursor c_limite is',
'select CLEM_MON,',
'       CLEM_IMP_LIM_CR,',
'       CLEM_BLOQ_LIM_CR,',
'       CLEM_PLAN_FINAN,',
'       CLEM_PORC_DESCUENTO,',
'       CLEM_NRO_LISTA_PRECIO,',
'       CLEM_RECARGO,',
'       CLEM_PORC_INTERES_MOROSIDAD',
'from fin_cli_empresa, gen_razon_social r',
'where clem_cli = r.rso_codigo',
'and clem_empr = :P_EMPRESA_ID',
'and r.rso_empr = :P_EMPRESA_ID',
'and r.rso_codigo = :P20_RSO_CODIGO;',
'',
'BEGIN',
'if apex_collection.collection_exists(p_collection_name => ''FINM01_AGREGAR_LIMITE_CREDITO'') then',
'   APEX_COLLECTION.DELETE_COLLECTION(P_COLLECTION_NAME => ''FINM01_AGREGAR_LIMITE_CREDITO'');',
'end if;   ',
'if not apex_collection.collection_exists(p_collection_name => ''FINM01_AGREGAR_LIMITE_CREDITO'') then',
'        APEX_COLLECTION.CREATE_COLLECTION(P_COLLECTION_NAME => ''FINM01_AGREGAR_LIMITE_CREDITO'');',
'    end if;',
'      ',
'FOR r IN c_limite LOOP   ',
'     apex_collection.add_member(',
'            p_collection_name => ''FINM01_AGREGAR_LIMITE_CREDITO'', ',
'            p_n001 => r.CLEM_MON,',
'            p_n002 => r.CLEM_IMP_LIM_CR,',
'            p_c001 => r.CLEM_BLOQ_LIM_CR,',
'            p_n004 => r.CLEM_PLAN_FINAN,',
'            p_n005 => r.CLEM_PORC_DESCUENTO,',
'            p_c006 => r.CLEM_NRO_LISTA_PRECIO,',
'            p_c007 => r.CLEM_RECARGO,',
'            p_c008 => r.CLEM_PORC_INTERES_MOROSIDAD',
'        );',
'      end loop;      ',
'',
'END;'))
,p_attribute_02=>'P104_OP_CTA_OPER,P104_OP_CTA_CTA_CODIGO'
,p_wait_for_result=>'Y'
);
end;
