wwv_flow_api.create_page_da_action(
 p_id=>wwv_flow_api.id(7656650838044478)
,p_event_id=>wwv_flow_api.id(7656291086044456)
,p_event_result=>'TRUE'
,p_action_sequence=>10
,p_execute_on_page_init=>'N'
,p_action=>'NATIVE_EXECUTE_PLSQL_CODE'
,p_attribute_01=>wwv_flow_string.join(wwv_flow_t_varchar2(
'declare',
'  v_nls_nc varchar2(1); -- Numeric characters',
'',
'begin',
'',
unistr('  -- Este select obtiene el separador de "miles" de la configuraci\00F3n de formato num\00E9rico de la sesi\00F3n.'),
unistr('  -- El primer s\00EDmbolo separa los decimales'),
unistr('  -- Y el segundo s\00EDmbolo separa los grupos de millares'),
'  -- Si es ,.   significa que el formato es: 1.250,25',
'  -- Si es .,   significa que el formato es: 1,250.25',
unistr('  -- Dependiendo de la configuraci\00F3n, debemos reemplazar las compas o los puntos'),
'',
'  -- Recuperamos el segundo caracter para reemplazarlo',
'  select substr(value, 2, 1)',
'    into v_nls_nc',
'    from v$nls_parameters',
'   where parameter = ''NLS_NUMERIC_CHARACTERS'';',
'',
'  :p20_rso_tipo_salario  := replace(:p20_rso_tipo_salario, v_nls_nc);',
'  :p20_rso_salario_base  := replace(:p20_rso_salario_base, v_nls_nc);',
'  :p20_rso_imp_hora_n_n  := replace(:p20_rso_imp_hora_n_n, v_nls_nc);',
'  :p20_rso_imp_hora_n_d  := replace(:p20_rso_imp_hora_n_d, v_nls_nc);',
'  :p20_rso_imp_hora_e_d  := replace(:p20_rso_imp_hora_e_d, v_nls_nc);',
'  :p20_rso_imp_hora_e_n  := replace(:p20_rso_imp_hora_e_n, v_nls_nc);',
'  :p20_rso_imp_hora_df_d := replace(:p20_rso_imp_hora_df_d, v_nls_nc);',
'  :p20_rso_imp_lleg_hora := replace(:p20_rso_imp_lleg_hora, v_nls_nc);',
'  :p20_rso_salario_total := replace(:p20_rso_salario_total, v_nls_nc);',
'  perm001.cargar_salarios(:p_empresa_id,',
'                          :p20_rso_codigo,',
'                          :p20_rso_tipo_salario,',
'                          :p20_rso_salario_base,',
'                          :p20_rso_imp_hora_n_n,',
'                          :p20_rso_imp_hora_n_d,',
'                          :p20_rso_imp_hora_e_d,',
'                          :p20_rso_imp_hora_e_n,',
'                          :p20_rso_imp_hora_df_d,',
'                          :p20_rso_imp_lleg_hora,',
'                          :p20_rso_Salario_total);',
'end;',
''))
,p_attribute_02=>'P20_RSO_TIPO_SALARIO,P20_RSO_SALARIO_BASE,P20_RSO_IMP_HORA_N_N,P20_RSO_IMP_HORA_N_D,P20_RSO_IMP_HORA_E_D,P20_RSO_IMP_HORA_E_N,P20_RSO_IMP_HORA_DF_D,P20_RSO_SALARIO_TOTAL,P20_RSO_CODIGO'
,p_attribute_03=>'P20_RSO_IMP_HORA_N_N,P20_RSO_IMP_HORA_N_D,P20_RSO_IMP_HORA_E_D,P20_RSO_IMP_HORA_E_N,P20_RSO_IMP_HORA_DF_D,P20_RSO_IMP_LLEG_HORA,P20_RSO_SALARIO_TOTAL'
,p_attribute_04=>'N'
,p_wait_for_result=>'Y'
